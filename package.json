{
  "name": "twiphy",
  "version": "0.0.4",
  "description": "Puttin' my GIFs all up in yo Twitch.",
  "main": "index.js",
  "author": "colshacol <colshacol@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "nodemon",
    "pack": "webpack",
    "show--scripts": "jq '{scripts}' package.json",
    "parser": "node proto && jq '{program.body}' parsed.json",
    "prettyjson": "prettyjson --indent=4 proto/parsed.json"
  },
  "babel": {
    "plugins": [
      [
        "flow-runtime",
        {
          "assert": true,
          "annotate": true
        }
      ],
      "transform-dev",
      "transform-decorators-legacy",
      "transform-class-properties",
      "react-html-attrs",
      "jsx-control-statements",
      [
        "react-css-modules",
        {
          "generateScopedName": "_[name]-[local]",
          "context": ".${PWD}/client"
        }
      ],
      "transform-do-expressions"
    ],
    "presets": [
      "stage-0",
      "es2015",
      "react"
    ]
  },
  "devDependencies": {
    "acorn": "^5.1.1",
    "autoprefixer": "^6.7.7",
    "babel": "^6.23.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-flow-runtime": "^0.10.0",
    "babel-plugin-react-css-modules": "^3.0.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-dev": "^2.0.0",
    "babel-plugin-transform-do-expressions": "^6.22.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babylon": "^6.17.4",
    "cq-prolyfill": "^0.3.3",
    "css-loader": "^0.28.0",
    "espree": "^3.4.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "flow-runtime": "^0.10.0",
    "format-json-pretty": "^1.0.1",
    "json-formatter-js": "^2.2.0",
    "jsx-control-statements": "^3.1.5",
    "mobx-devtools": "^0.0.7-alpha-8",
    "node-jq": "^0.5.4",
    "nodemon": "^1.11.0",
    "postcss": "^5.2.16",
    "postcss-loader": "^2.0.5",
    "regenerator-runtime": "^0.10.5",
    "style-loader": "^0.16.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "cors": "^2.8.3",
    "express": "^4.15.2",
    "kind-of": "^5.0.0",
    "mobx": "^3.1.8",
    "mobx-react": "^4.1.5",
    "prettyjson": "^1.2.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "3.0.5",
    "styled-components": "^2.1.1",
    "wait-for-element": "^1.0.2"
  },
  "config": {
    "LOCAL_IDENT_NAME": "[name]-[local]"
  }
}
